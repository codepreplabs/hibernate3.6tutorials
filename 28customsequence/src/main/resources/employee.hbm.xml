<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.codeprep.entity.Employee" table="employee">
        <id name="employeeId">
            <generator
                    class="com.codeprep.sequence.StringSequenceIdentifier">
                <param name="sequence">SEQ_ALERTS</param>
                <param name="max_lo">99</param>
            </generator>
        </id>
        <property name="name"></property>
        <property name="email"></property>
    </class>
</hibernate-mapping>